<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.termsetter">

    <uses-permission android:name="android.permission.WAKE_LOCK" />



    <application
        android:allowBackup="true"
        android:icon="@drawable/ts_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ts_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">

        <!--The default activity is set to Login Page (if otherwise, please check your Edit configuration)-->

        <!--Mika-->
        <activity
            android:name="com.example.comp3350.termsetter.Logic.AccountSettings"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.AccountManagementMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.AccountManagementMenu"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Logic.AccountChangePassword"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.AccountManagementMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.AccountManagementMenu"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Logic.AccountUpdateEmail"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.AccountManagementMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.AccountManagementMenu"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.AccountManageProfile"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.AccountManagementMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.AccountManagementMenu"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.AccountManagementMenu"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.TempNavigationLinks">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.TempNavigationLinks"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.TempNavigationLinks"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.MainActivity"/>
        </activity>

        <uses-library
            android:name="com.google.android.wearable"
            android:required="false" />
        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->
        <meta-data
            android:name="com.google.android.wearable.standalone"
            android:value="true" />

        <!-- Kevin
        Login Page (Parent)
       Create Account (Child)
        -->
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.LoginPage"
            android:label="@string/title_activity_login_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Persistence.CreateAccount"
            android:label="@string/title_activity_create_account"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.OfferedClassesView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.LoginPage" />
        </activity>

        <!--
        Andrea
        -->
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.OfferedClassesDetail"
            android:label="@string/title_activity_offered_classes_detail"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.OfferedClassesView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.OfferedClassesView" />
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.OfferedClassesView"
            android:label="@string/title_activity_offered_classes_view"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.OfferedClassesCategories">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.OfferedClassesCategories" />
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.OfferedClassesCategories"
            android:parentActivityName="com.example.comp3350.termsetter.Presentation.TempNavigationLinks">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.comp3350.termsetter.Presentation.TempNavigationLinks"/>
        </activity>
        <activity
            android:name="com.example.comp3350.termsetter.Presentation.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>